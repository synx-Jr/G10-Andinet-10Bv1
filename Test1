let time = 20;
if (time < 12) {
  console.log("Good Morning");
} else if (time > 11 && time <=20) {
  console.log("Good evening");
} 
else if(time>12 && time>=10){
  console.log("Good day");
}
let x=30 //where x is seconds
if(x<30) {
console.log("less than half a minute")
}else if(x=30) {
console.log("half a minute") 
}else if(x>30 & x<=60) {
console.log("almost a minute")
}
